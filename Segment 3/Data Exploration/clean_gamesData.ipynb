   "source": [
    "# Make new 'franchise by tier' column either by binning scores or sales\n",
    "def tier_group(Critic_Score):\n",
    "    if Critic_Score >= 90:\n",
    "        return 'Tier 1'\n",
    "    elif Critic_Score >= 80:\n",
    "        return 'Tier 2'\n",
    "    elif Critic_Score >= 70:\n",
    "        return 'Tier 3'\n",
    "    elif Critic_Score >= 60:\n",
    "        return 'Tier 4'\n",
    "    elif Critic_Score >= 50:\n",
    "        return 'Tier 5'\n",
    "    else:\n",
    "        return 'Other'\n",
    "\n",
    "df = df.dropna(subset=['Critic_Score']).reset_index(drop=True)\n",
    "df['Tier by Score'] = df['Critic_Score'].apply(lambda x: tier_group(x))\n"
