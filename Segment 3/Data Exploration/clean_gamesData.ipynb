   "source": [
    "# Get first release year for franchises\n",
    "first_year = df.groupby('Franchise')['Release Year'].min().reset_index()\n",
    "first_year = first_year.rename(columns = {'Release Year': 'First Release Year'})\n",
    "df = first_year.merge(df, on ='Franchise')\n",
    "df"
   ]
